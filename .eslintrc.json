{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "rules": {
      "require-jsdoc": 0,
      "object-curly-spacing": ["error", "always"],
      "arrow-parens": ["error", "as-needed"],
      "indent": ["error", 2],
      "quotes": [2, "double", { "avoidEscape": true }]
  },
  "extends": "google"
}
